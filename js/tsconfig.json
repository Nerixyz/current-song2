{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "moduleResolution": "Node"
  },
  "exclude": [
    "node_modules"
  ]
}
